{
  "name": "@ukyo-cli/core",
  "version": "2.0.0-alpha.11",
  "description": "Webpack 4 - SCSS - PUG boilerplate",
  "main": "index.js",
  "loglevel": "silent",
  "bin": {
    "ukyo-core": "./bin/ukyo-core.js"
  },
  "scripts": {
    "build": "npm run clean -s && npm run production -s",
    "clean": "rimraf public",
    "dev": "node ./webpack/webpack.dev.js",
    "production": "cross-env webpack --progress --hide-modules --config ./webpack/webpack.prod.js",
    "start": "npm run dev",
    "create-stats": "webpack --config ./webpack/webpack.prod.js --profile --json > ./bundle/stats.json",
    "read-stats": "webpack-bundle-analyzer ./bundle/stats.json ./public",
    "bundle-analyzer": "npm run create-stats && npm run read-stats"
  },
  "keywords": [
    "webpack 4",
    "boilerplate",
    "scss",
    "pug",
    "babel"
  ],
  "author": "Giulio Collesei",
  "license": "ISC",
  "browserslist": [
    "last 2 version",
    "> 1%",
    "not dead"
  ],
  "dependencies": {
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "babel-eslint": "10.1.0",
    "babel-loader": "8.1.0",
    "copy-webpack-plugin": "6.1.1",
    "core-js": "3",
    "cross-env": "7.0.2",
    "css-loader": "4.3.0",
    "eslint": "7.9.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-babel": "5.3.1",
    "eslint-plugin-compat": "3.8.0",
    "eslint-plugin-prettier": "^3.1.4",
    "extract-loader": "5.1.0",
    "file-loader": "6.1.0",
    "html-loader": "1.3.1",
    "html-webpack-harddisk-plugin": "1.0.2",
    "html-webpack-plugin": "4.5.0",
    "ip": "1.1.5",
    "mini-css-extract-plugin": "0.11.2",
    "node-sass": "4.14.1",
    "node-sass-json-importer": "4.3.0",
    "open": "^7.2.1",
    "optimize-css-assets-webpack-plugin": "5.0.4",
    "path": "0.12.7",
    "postcss-loader": "4.0.2",
    "prettier": "^2.1.2",
    "prettier-eslint": "^11.0.0",
    "pug": "3.0.0",
    "pug-html-loader": "1.1.5",
    "pug-loader": "2.4.0",
    "raw-loader": "4.0.1",
    "rimraf": "3.0.2",
    "sass-loader": "10.0.2",
    "sass-mq": "5.0.1",
    "source-map-loader": "1.1.0",
    "style-loader": "1.2.1",
    "stylelint": "13.7.1",
    "stylelint-config-recommended": "3.0.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.18.0",
    "svg-loader": "^0.0.2",
    "uglifyjs-webpack-plugin": "2.2.0",
    "url-loader": "^4.1.0",
    "webpack": "4.44.2",
    "webpack-bundle-analyzer": "^3.9.0",
    "webpack-cli": "3.3.12",
    "webpack-dev-server": "3.11.0",
    "webpack-format-messages": "2.0.6",
    "webpack-merge": "5.1.4",
    "webpack-messages": "2.0.4",
    "whatwg-fetch": "3.4.1"
  },
  "gitHead": "f33b4b94f961c12a6aa8b6110c7339c6f1285193"
}
